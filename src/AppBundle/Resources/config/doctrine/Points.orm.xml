<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Points" table="Points">
    <indexes>
      <index name="fk_Points_Players_idx" columns="Players_id"/>
      <index name="fk_Points_Games1_idx" columns="Games_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="points" type="integer" column="points" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-one field="games" target-entity="Games" fetch="LAZY">
      <join-columns>
        <join-column name="Games_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="players" target-entity="Players" fetch="LAZY">
      <join-columns>
        <join-column name="Players_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
